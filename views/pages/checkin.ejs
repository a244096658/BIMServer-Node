<!-- views/checkin.ejs -->
<!doctype html>
<html>
	<% include ../partials/header %>
	<body>
        <section name="jumbotron"  >
            <div class="jumbotron text-center">
                <h2>BIMServer - Node</h2>
				<% if(typeof messageUserType !=="undefined"){ %>
					<%- include('../partials/navigator',{userType:messageUserType}) %>
					
				
            </div>
        </section>

			<% if(typeof message !=="undefined"){ %>
				<% if(message.length>0){ %>
					<div class="alert alert-success"><%= message %></div>
				<% }%>
			<% }%>

		<section name="content">
			<div class="container" >
				<% if(typeof moduleName!=="undefined"){  %>
					<% for(var module in moduleName){  %>
						<%- include(moduleName[module]) %>
					<% } %>
				<% } %>

				<% } %>
			</div>
		</section>

		<% console.log(messageUserType) %>

	</body>
</html>