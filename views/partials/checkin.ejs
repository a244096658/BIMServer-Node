<!-- checkinIFC FORM -->
<div id="checkinIFC">
    <form  ref='uploadForm'  id='uploadForm'   action="/checkin" method="post" encType="multipart/form-data">
        <div class="form-group">
            <label>poid</label>
            <input type="text" class="form-control" name="poid">

            <label>comment</label>
            <input type="text" class="form-control" name="comment">

            <label>file</label>
            <input type="file" class="form-control" name="IFCfile">

            <label>merge</label>
            <input type="hidden" name="merge" value="false">
            <input type="checkbox" name="merge" value="true"><br>

            <label>sync</label>
            <input type="hidden" name="sync" value="false">
            <input type="checkbox" name="sync" value="true"><br>

        </div>
            <input type="submit" class="btn btn-warning " value="checkin">
    </form>
</div>